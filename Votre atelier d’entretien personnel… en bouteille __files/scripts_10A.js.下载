jQuery(function(e){function n(e,n,t,o){var i=void 0!=window.screenLeft?window.screenLeft:screen.left,c=void 0!=window.screenTop?window.screenTop:screen.top;width=window.innerWidth?window.innerWidth:document.documentElement.clientWidth?document.documentElement.clientWidth:screen.width,height=window.innerHeight?window.innerHeight:document.documentElement.clientHeight?document.documentElement.clientHeight:screen.height;var d=width/2-t/2+i,s=height/2-o/2+c,r=window.open(e,n,"scrollbars=yes, width="+t+", height="+o+", top="+s+", left="+d);window.focus&&r.focus()}function t(){for(var e=document.getElementsByTagName("a"),t=0;t<e.length;t++){var o=e[t];o.className.indexOf("fb_btn")+1&&(o.onclick=function(){n("https://www.facebook.com/sharer/sharer.php","TIP OF THE WEEK: How this little device saves you from THOUSANDS of dollars spent at crooked repair shops...","640","380")}),o.className.indexOf("tw_btn")+1&&(o.onclick=function(){n("https://twitter.com/share","","640","380")}),o.className.indexOf("gp_btn")+1&&(o.onclick=function(){n("https://plus.google.com/share","","640","380")})}}e("#sidebar-scroll").scrollToFixed({}),window.onload=function(){t()}});